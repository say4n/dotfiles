[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
[user]
	name = Sayan Goswami
	email = goswami.sayan47@gmail.com
[core]
	editor = subl -n -w

[alias]
 	co = checkout
 	ci = commit
	st = status
  	br = branch
  	hist = log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=short
  	type = cat-file -t
	dump = cat-file -p

